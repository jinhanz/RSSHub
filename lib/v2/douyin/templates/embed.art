<video controls="controls" preload="none" referrerpolicy="no-referrer"
style="max-width:95vw;max-height:90vh;"
{{ if img }}
poster="{{ img }}"
{{ /if }}
{{ if duration }}
duration="{{ duration }}"
{{ /if }}
>
  {{ each videoList source }}
  <source referrerpolicy="no-referrer" src="{{ source }}">
  {{ /each }}
</video>
